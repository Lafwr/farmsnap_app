<div class="main-form-container">
  <div class="form-container">
    <h3>Create Farmer Profile:</h3>
      <%= simple_form_for @farmer, local: true do |f| %>
        <%= f.input :photo, as: :file, label: "Upload Your Photo" %>
        <%= f.input :bio, label: "Bio:" %>
        <%= f.input :location, label: "Location:" %>
        <%= f.submit "Create Profile", class: "btn btn-dark" %>
      <% end %>
  </div>
</div>
